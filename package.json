{
  "name": "@oasis/vite-plugin-react-md",
  "version": "0.1.0",
  "description": "Markdown for Vite",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm"
  },
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "keywords": [
    "vite",
    "vite-plugin",
    "markdown-it",
    "react",
    "react component"
  ],
  "author": "oasis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oasis-cloud/vite-plugin-react-md"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.5",
    "prettier": "^2.5.1",
    "tsup": "^5.11.9",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@rollup/pluginutils": "^4.1.2",
    "markdown-it": "^12.3.0"
  }
}
